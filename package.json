{
  "name": "backend-nusantara-edu",
  "version": "1.0.0",
  "description": "Backend API for NusantaraEdu Management Information System",
  "main": "dist/main.js",
  "scripts": {
    "dev": "tsx watch src/main.ts",
    "build": "tsc",
    "start": "node dist/main.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:seed-auth": "tsx prisma/seed-auth.ts",
    "prisma:seed-academic": "tsx prisma/seed-academic.ts",
    "prisma:seed-teacher-evaluation": "tsx prisma/seed-teacher-evaluation.ts",
    "prisma:seed-resource": "tsx prisma/seed-resource.ts",
    "prisma:seed-kpi": "tsx prisma/seed-kpi.ts",
    "prisma:reset": "prisma migrate reset && npm run prisma:seed",
    "clean": "rm -rf node_modules package-lock.json && npm install",
    "test:gemini": "tsx src/test/testGemini.ts",
    "ai:test": "npm run test:gemini",
    "ai:health": "tsx -e \"import('./src/services/geminiService.js').then(s => s.geminiService.healthCheck().then(r => console.log(r)))\"",
    "test:academic": "tsx src/test/testAcademicAnalysis.ts",
    "ai:academic": "npm run test:academic",
    "ai:full": "npm run test:ai-recommendations",
    "test:ai-recommendations": "tsx src/test/testAIRecommendationFull.ts"

  },
  "keywords": [
    "nodejs",
    "typescript",
    "prisma",
    "express",
    "education"
  ],
  "author": "NusantaraEdu Team",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@prisma/client": "^5.9.1",
    "@types/multer": "^1.4.13",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.12.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.1",
    "path-to-regexp": "^6.2.1",
    "pdf-lib": "^1.17.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.5",
    "prisma": "^5.9.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
